{
  "name": "Unshorter",
  "version": "0.0.7",
  "description": "Покажет реальное содержание короткой ссылки с любого сайта. Предупредит об опасности.",
  "permissions": [
    "http://unshort.ru/*"
  ],
  "icons": {
    "48" : "unshort.png"
  },
  "background_page" : "background.html",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js" : ["jquery-1.6.1.min.js","contentscript.js"]
    }
  ]
}
